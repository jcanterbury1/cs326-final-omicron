{
  "name": "finalproject",
  "type": "commonjs",
  "scripts": {
    "start": "node server/server.js",
    "lint": "eslint client/ server/"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "http-server": "^0.12.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "commonjs"
    },
    "rules": {
      "consistent-return": "error",
      "no-var": "error",
      "eqeqeq": "error",
      "semi": [
        "error",
        "always"
      ],
      "curly": [
        "warn",
        "all"
      ],
      "prefer-const": [
        "warn",
        {}
      ],
      "quotes": [
        2,
        "double",
        "avoid-escape"
      ]
    }
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "faker": "^5.1.0",
    "fs": "0.0.1-security",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg-promise": "^10.8.0",
    "url": "^0.11.0"
  }
}
